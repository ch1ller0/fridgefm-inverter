{
  "name": "@fridgefm/inverter-test",
  "version": "0.3.0",
  "description": "Testing library for @fridgefm/inverter",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "module": "build/index.js",
  "license": "MIT",
  "keywords": [
    "ioc",
